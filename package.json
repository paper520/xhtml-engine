    "start": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yelloxing/xhtml-engine.git"
  },
  "keywords": [
    "xhtml",
    "engine",
    "node.js"
  ],
  "author": "心叶",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yelloxing/xhtml-engine/issues"
  },
  "homepage": "https://github.com/yelloxing/xhtml-engine#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "cuf": "^1.2.0",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "terser": "^4.1.2"
  },
  "dependencies": {
    "@yelloxing/core.js": "^0.3.7"
  }
